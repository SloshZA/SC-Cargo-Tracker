{
  "name": "sc-cargo-tracker-react",
  "version": "1.0.0",
  "homepage": "https://sloshza.github.io/SC-Tracker-React/",
  "source": "index.html",
  "scripts": {
    "start": "parcel index.html",
    "clean": "if exist dist rd /s /q dist && if exist .parcel-cache rd /s /q .parcel-cache",
    "predeploy": "npm run clean && npm run build",
    "deploy": "gh-pages -d dist",
    "build": "parcel build index.html --dist-dir dist --public-url ./",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": ["star-citizen", "cargo-tracking", "gaming-tools", "react"],
  "author": "SloshZA",
  "license": "ISC",
  "description": "A comprehensive tool for managing and tracking cargo hauling missions in Star Citizen",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-color": "^2.19.3",
    "react-select": "^5.10.0",
    "react-tesseract": "^1.0.0",
    "react-window": "^1.8.11",
    "tesseract.js": "^6.0.0",
    "three": "^0.172.0"
  },
  "devDependencies": {
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "gh-pages": "^4.0.0",
    "parcel": "^2.0.0",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "string_decoder": "^1.3.0",
    "vm-browserify": "^1.1.2"
  },
  "repository": {
    "type": "git",
